{
  "name": "seedlot-selection-tool",
  "version": "1.0.0",
  "description": "Seedlot Selection Tool",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.config.babel.js",
    "build": "webpack --config webpack.config.babel.js --stats-children --node-env production",
    "lint": "eslint .",
    "merge-regions": "mapshaper -i ../source/sst/static/geometry/*.json combine-files -o ../source/sst/static/geometry/regions.topojson",
    "extract-messages": "ttag update locales/es_MX/sst-js.po src && msgattrib -o locales/es_MX/sst-js.po --clear-fuzzy locales/es_MX/sst-js.po"
  },
